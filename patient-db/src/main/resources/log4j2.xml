<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" packages="com.hartwig.hmftools.patientdb.log4j2plugins">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>